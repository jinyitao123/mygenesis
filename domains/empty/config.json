{
  "domain_id": "empty",
  "name": "空白项目",
  "description": "从零开始定义新的本体，适合自定义领域",
  "version": "1.0.0",
  "author": "Genesis Forge Team",
  "created_at": "2026-02-05",
  
  "ui_config": {
    "primary_color": "#6b7280",
    "secondary_color": "#9ca3af",
    "accent_color": "#d1d5db",
    "background_color": "#F5F5F5",
    "text_color": "#333333",
    "font_family": "Microsoft YaHei, sans-serif",
    "icon_set": "emoji"
  },
  
  "features": {
    "object_types": [],
    "action_types": [],
    "relation_types": [],
    "supported_csv_columns": ["id", "name", "type", "properties_json"]
  },
  
  "default_objects": [],
  
  "templates": {
    "csv_import": {
      "filename": "custom_data.csv",
      "columns": ["id", "name", "type", "properties_json"],
      "example": "entity_001,自定义实体,ENTITY,{\"custom_property\":\"value\"}"
    },
    "cypher_queries": {
      "find_all_entities": "MATCH (n) RETURN n.id, n.name, n.type",
      "find_by_type": "MATCH (n) WHERE n.type = $type RETURN n"
    }
  },
  
  "validation_rules": {}
}